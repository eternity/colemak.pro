{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "region": "us-central1",
      "minInstances": 0,
      "maxInstances": 5,
      "memory": "256MiB"
    }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
